{
  "name": "endeavour-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/big-little-things/endeavour-backend.git",
  "author": "Ryan Wire <ryanwire@outlook.com>",
  "license": "MIT",
  "scripts": {
    "start": "node ./build/server",
    "start:dev": "nodemon",
    "build": "rm -rf ./build && tsc --build",
    "lint": "eslint './src/**/*.ts' --quiet",
    "create-env": "printenv > .env",
    "migrate": "sequelize db:migrate",
    "reset:db": "sequelize db:migrate:undo:all && npm run migrate",
    "test": "NODE_ENV=test npm run reset:db && NODE_ENV=test nyc mocha --require ts-node/register --timeout 10000 src/tests/**/*.spec.ts --exit",
    "coveralls-coverage": "nyc report --reporter=text-lcov | coveralls",
    "codecov-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov --disable=gcov",
    "coverage": "nyc report --reporter=lcov && npm run coveralls-coverage"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^5.1.4",
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.0.1",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.4",
    "socket.io": "^4.5.1",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/passport": "^1.0.10",
    "@types/passport-jwt": "^3.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.9.1",
    "typescript": "4.6.4"
  }
}
