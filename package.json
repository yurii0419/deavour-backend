{
  "name": "endeavour-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/big-little-things/endeavour-backend.git",
  "author": "Ryan Wire <ryanwire@outlook.com>",
  "license": "MIT",
  "scripts": {
    "start": "node ./build/server",
    "start:dev": "nodemon",
    "build": "rm -rf ./build && tsc --build",
    "lint": "eslint './src/**/*.ts' --quiet",
    "create-env": "printenv > .env",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "reset:db": "sequelize db:migrate:undo:all && npm run migrate",
    "seed": "sequelize db:seed:all",
    "test": "NODE_ENV=test npm run reset:db && NODE_ENV=test npm run seed && NODE_ENV=test nyc mocha --require ts-node/register --timeout 25000 src/tests/**/*.spec.ts --exit",
    "coveralls-coverage": "nyc report --reporter=text-lcov | coveralls",
    "codecov-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov --disable=gcov",
    "coverage": "nyc report --reporter=lcov && npm run coveralls-coverage"
  },
  "nyc": {
    "extension": [
      ".js",
      ".ts"
    ],
    "include": [
      "src/"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/*.d.js",
      "**/*.spec.js",
      "coverage",
      "dist",
      "build",
      "src/tests",
      "src/migrations",
      "src/seeders",
      ".eslintrc.js",
      "src/server/*",
      "src/utils/socket.ts",
      "src/constants",
      "**/wawiAPI"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^5.1.4",
    "@google-cloud/pubsub": "^3.3.0",
    "@sendgrid/eventwebhook": "^7.7.0",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.0.1",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "firebase-admin": "^11.5.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.4",
    "short-uuid": "^4.2.2",
    "socket.io": "^4.5.1",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/passport": "^1.0.10",
    "@types/passport-jwt": "^3.0.6",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@types/sinon-express-mock": "^1.3.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "codecov": "^3.8.3",
    "coveralls": "^3.1.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^10.0.0",
    "nodemon": "3.1.7",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.4.1",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "sinon-express-mock": "^2.2.1",
    "ts-node": "^10.9.1",
    "typescript": "4.6.4"
  }
}
